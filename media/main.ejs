<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- styles -->
    <link href="<%- rootUri %>/css/normalize-8.0.1.css" rel="stylesheet">
    <link href="<%- rootUri %>/css/fa-6.2.0.min.css" rel="stylesheet">
    <link href="<%- rootUri %>/css/bootstrap-5.2.1.min.css" rel="stylesheet">
    <!-- <link href="<%- rootUri %>/css/bootstrap-<%- colorMode %>.min.css" rel="stylesheet"> -->
    <link href="<%- rootUri %>/css/main.css" rel="stylesheet">

    <!-- CodeMirror -->
    <link href="<%- rootUri %>/css/codemirror-6.65.7.min.css" rel="stylesheet">

    <title><%= title %></title>
  </head>

  <body>
    <div id="vscode-kanban-board"></div>

    <!-- utils -->
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/lodash-4.17.21.min.js"></script>
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/ajv7-8.11.0.bundle.min.js"></script>
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/i18next-21.9.2.min.js"></script>

    <!-- CodeMirror -->
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/codemirror-6.65.7.min.js"></script>
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/codemirror/markdown-6.65.7.min.js"></script>
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/codemirror/markdown-fold-6.65.7.min.js"></script>

    <!-- React & Co. -->
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/react-17.0.2.js"></script>
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/react-dom-17.0.2.js"></script>
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/babel-7.18.13.js"></script>
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/react-bootstrap-2.5.0.min.js"></script>

    <!-- scripts -->
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/main.js"></script>
    <script nonce="<%- nonce %>" src="<%- rootUri %>/js/main.<%- colorMode %>.js"></script>

    <!-- JSX components -->
    <% jsxComponentUris.forEach(function(jsxUri) { %>
      <script nonce="<%- nonce %>" src="<%- jsxUri %>" type="text/babel"></script>
    <% }); %>
  </body>
</html>